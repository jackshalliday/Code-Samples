<table class="table table-bordered">
  <tr>
    <th>Username</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Posts</th>
    <th>Mod</th>
    <th>Admin</th>
  </tr>
  <tbody>
    <% @users.each do |user| %>
        <tr>
          <td> <%= user.username %> </td>
          <td> <%= user.first_name %> </td>
          <td> <%= user.last_name %> </td>
          <td> <%= user.email %> </td>
          <td> <%= get_num_of_posts(user) %></td>
          <td> <%= user.is_moderator? %> </td>
          <td> <%= user.is_admin? %> </td>
        </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @users %>